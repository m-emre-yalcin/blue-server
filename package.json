{
  "name": "blue-server-api",
  "version": "0.3.10",
  "description": "Create an abstracted and extendible server from one JSON file!",
  "files": [
    "/*"
  ],
  "scripts": {
    "dev": "nodemon ./api/index --dev",
    "start": "nodemon ./api/index --dev",
    "build": "webpack --config webpack.config.js && npm run copy-dependency-files",
    "copy-dependency-files": "copy \"package.json\" \"dist\" && copy \"LICENSE\" \"dist\" && copy \"README.md\" \"dist\" && copy \"ROADMAP.md\" \"dist\"",
    "publish": "npm run build && npm run copy-dependency-files && cd \"dist\" && npm publish",
    "db:up": "knex migrate:up --knexfile database/config.js",
    "db:down": "knex migrate:down --knexfile database/config.js",
    "db:make": "knex migrate:make db --knexfile database/config.js",
    "db:latest": "knex migrate:latest --knexfile database/config.js",
    "db:rollback": "knex migrate:rollback --knexfile database/config.js",
    "db:list": "knex migrate:list --knexfile database/config.js",
    "db:seed": "knex seed:run --knexfile database/config.js",
    "db:res": "npm run db:down && npm run db:up && npm run db:seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-emre-yalcin/blue-server.git"
  },
  "author": {
    "name": "M. Emre Yalcin",
    "email": "emrreyalcin@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m-emre-yalcin/blue-server/issues"
  },
  "homepage": "https://github.com/m-emre-yalcin/blue-server",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "firebase": "^9.6.1",
    "formidable": "^2.0.1",
    "ipx": "^0.9.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.14",
    "languagedetect": "^2.0.0",
    "pg": "^8.7.1",
    "response-time": "^2.3.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "typescript": "^4.3.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "keywords": [
    "api",
    "server",
    "create",
    "server",
    "auto",
    "server",
    "generator"
  ]
}